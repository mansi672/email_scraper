{
  "manifest_version": 2,
  "name": "Email Scraper",
  "description": "Email Scraper Chrome extension automates the extraction of email addresses from web pages.",
  "version": "1.0.0",
  "icons": {
    "128": "Prabisha logo.png"
   
  },
  "browser_action": {
    "default_icon": "Prabisha logo.png",
    "default_popup": "popup.html",
    "default_title" : "Email Scraper"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/scripts/fontawesome.js", "/scripts/page.js"
      ]
    }
  ],
  "background":
  {
    "scripts":["/scripts/utils.js", "/scripts/common.js", "/scripts/background.js"]
  }
}